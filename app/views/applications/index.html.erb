<p id="notice"><%= notice %></p>

<h1>Listing Applications</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Location</th>
      <th>Age</th>
      <th>Reason for interest</th>
      <th>Educational background</th>
      <th>Employment background</th>
      <th>Code experience</th>
      <th>Foreign languages</th>
      <th>Travel experience</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @applications.each do |application| %>
      <tr>
        <td><%= application.first_name %></td>
        <td><%= application.last_name %></td>
        <td><%= application.email %></td>
        <td><%= application.phone %></td>
        <td><%= application.location %></td>
        <td><%= application.age %></td>
        <td><%= application.reason_for_interest %></td>
        <td><%= application.educational_background %></td>
        <td><%= application.employment_background %></td>
        <td><%= application.code_experience %></td>
        <td><%= application.foreign_languages %></td>
        <td><%= application.travel_experience %></td>
        <td><%= link_to 'Show', application %></td>
        <td><%= link_to 'Edit', edit_application_path(application) %></td>
        <td><%= link_to 'Destroy', application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application', new_application_path %>
